<?xml version="1.0" encoding="UTF-8"?>
<Recipe xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="recipe.xsd">
 
  <Assignment>
    <!-- Create a TC variable with the filename of ASCII data log -->
     <Set name="TC.datalogPath"
      value="${ENV.HOME}/datalog_${ENV.HOSTNAME}_${ENV.LOGNAME}_${TC.LOT_ID}_${ENV.TIMESTAMP}" />
  </Assignment>
</Recipe>